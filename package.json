{
  "name": "@sscfaith/vue-bpmn",
  "version": "0.0.2",
  "author": {
    "name": "SSC",
    "url": "https://github.com/sscfaith",
    "email": "273702440@qq.com"
  },
  "main": "lib/index.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name VueBpmn --dest lib --filename index --formats umd-min ./packages/index.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bpmn-js": "^7.3.0",
    "bpmnlint": "^6.4.0",
    "monaco-editor": "^0.21.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.10.0",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "lib"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/sscfaith/vue-bpmn"
  },
  "keywords": [
    "vue",
    "element-ui",
    "bpmn",
    "bpmn.js",
    "flow",
    "flowable",
    "activiti",
    "camunda"
  ],
  "license": "MIT"
}
